b=int(input('b='))
if a>b:
    print(a,b)
elif a<b:
    print(b,a)
else:
    print('Ular teng')    

#9    
a=float(input('a='))
b=float(input('b='))
if a>b:
    a,b=b,a   
elif a==b:
    print('Ular teng')  

print("a={}, b={}".format(a,b))
#print("a=",a,"b=",b)


    
#10
a=int(input('a='))
b=int(input('b='))
if a!=b:
    a=b=a+b
else:
    a=b=0
print("a=",a,"b=",b)    

#11
a=int(input('a='))
b=int(input('b='))
if a!=b:
    if a>b:
        b=a
    else:
        a=b        
else:
    a=b=0
print("a=",a,"b=",b) 

#12
