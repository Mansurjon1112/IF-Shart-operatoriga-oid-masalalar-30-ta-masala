#1-usul
if a>b:
    kichik=b
    katta=a
else:
    kichik=a
    katta=b
if kichik>c:
    kichik=c
if katta<c:
    katta=c
print(kichik,katta)  
#2-usul 
if a>b>c:
    print(a,c)
elif (b>c>a):
    print(b,a)
elif a>c>b:
    print(a,b)
elif b>a>c:
    print(b,c)    
elif c>a>b:
    print(c,b)
else:
    print(c,a)  
