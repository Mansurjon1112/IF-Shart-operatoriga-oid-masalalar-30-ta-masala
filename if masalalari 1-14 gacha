#1
a=int(input('Son kiriting: '))
if a>0:
    a+=1
    print(a)
else:
    print(a)
    
#2
a=int(input('Son kiriting: '))
if a>0:
    a+=1
else:
    a-=2
print(a)    

#3 
a=int(input('Son kiriting: '))
if a>0:
    a+=1
elif a<0:
    a-=2
else: #a==0 
    a=10   
print(a)    

#4
a=int(input('a='))
b=int(input('b='))
c=int(input('c='))
sanoq=0
if a>0:
    sanoq+=1
if b>0:
    sanoq+=1
if c>0:
    sanoq+=1    
print(sanoq)

#5
a=int(input('a='))
b=int(input('b='))
c=int(input('c='))
musbat = 0 
manfiy = 0
if a>0:
    musbat+=1
elif a<0:
    manfiy+=1
    
if b>0:
    musbat+=1
elif b<0:
    manfiy+=1
    
if c>0:
    musbat+=1
elif c<0:
    manfiy+=1
print("Kiritilgan sonlarning {} tasi musbat, {} tasi manfiy".format(musbat,manfiy))

